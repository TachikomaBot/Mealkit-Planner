{
  "name": "meal-kit-app",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "import:recipes": "npx tsx scripts/import-recipes.ts",
    "import:recipes:test": "npx tsx scripts/import-recipes.ts recipes_w_search_terms.csv 100",
    "import:recipes:dinner": "npx tsx scripts/import-recipes.ts -c dinner -l 10000 -o public/data/recipes-dinner.json",
    "import:recipes:meals": "npx tsx scripts/import-recipes.ts -c meals -l 20000 -o public/data/recipes.json",
    "import:recipes:all": "npx tsx scripts/import-recipes.ts -l 50000 -o public/data/recipes.json"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "dexie": "^4.0.8",
    "dexie-react-hooks": "^1.1.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.28.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/node": "^24.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.11.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.20.6",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^0.20.5",
    "workbox-window": "^7.1.0"
  }
}
